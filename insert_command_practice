---
create table table_5
(
    product_id number not null,
    name varchar(50),
    product_cost number(5,2),
    product_retail number(5,2),
    product_type varchar(10),
    store_id number not null,
    constraint product_pk primary key(product_id,store_id)
)
--- with primary key constrainst where priamry key is combination of two column

insert all
     into table_2 (product_id,name,product_cost,product_retail,product_type,store_id) values(2,'subhash sharma','200.20',605.05,'daily_use',06)
